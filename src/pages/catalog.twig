        ] %}#}

        {% for i in 0..4 %}
        {% for item in item_catalog  %}
        <div class="item">
            <img src="{{ item.src }}" alt="">
            <p class="title"><a href="">{{ item.title }}</a></p>
            <p class="subcategory">{{ item.subcategory }}</p>
            <p class="price">{{ item.price }}</p>
        </div>
        {%  endfor %}
        {%  endfor %}
    </div>
    <div class="wrapper_pages_numbers">
        <p class="title">Страницы</p>
        <div class="js_pages_numbers">
            {#Заглушка#}
            <a href="" class="js_active_page">1</a>
        </div>
    </div>
</div>

